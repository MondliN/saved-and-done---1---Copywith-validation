<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="Bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css" />

    <style>
        body {
            background-image: url("paper.gif");
            background-color: #cccccc;
        }
    </style>

    <style type="text/css">
        .table-hover tbody tr:hover td {
            background-color: white;
            color: black;
        }

        body {
            margin: 50px;
        }
    </style>

    <script type="text/javascript">
        $("[data-toggle=popover]").popover({
            html: true,
            content: function () {
                return $('#popover-content').html();
            }
        });
    </script>
</head>
<body>
    @*<div class="jumbotron  jumbotron-fluid" >            
    </div>*@
    <div class="container-fluid">   
       @*@Html.ActionLink("Subsidiaries Capture System", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@ 
        <ul class="nav navbar-nav">       
         </ul>
        <ul class="nav navbar-nav navbar-right">
            @*<li><a href="RegistrationForm.aspx" dir="ltr"><span class="glyphicon glyphicon-user"></span> Sign In</a></li>*@
            <li><a class="active" data-toggle="tab" onclick="@("window.location.href='" + Url.Action("Login", "Home") + "'")"><span class="glyphicon glyphicon-log-in"> Login</> </a></li>
        </ul>

    </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">

                </ul>
            </div>
    <div class="container">
        <p>
            <input type="button" value="Print this page" onclick="window.print()" /> <span class="glyphicon glyphicon-print"></span> 
        </p>
        @RenderBody()

    </div>
    <hr />
    <footer>
        <div class="panel panel-primary">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </div>
    </footer>





    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script>
        $("[data-toggle=popover]").popover({
            html: true,
            content: function () {
                return $('#popover-content').html();
            }
        });
    </script>
</body>
</html>